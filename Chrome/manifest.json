{
    "name": "__MSG_name__",
    "version": "0.0.2.522",
    "default_locale": "en",
    "description": "__MSG_description__",
    "background_page": "background.html",
    "options_page": "options.html",
    "update_url": "http://clients2.google.com/service/update2/crx",
    
    "permissions": 
    [
        "tabs",
        "http://*/*"
    ],

    "plugins":
    [
        { "path": "plugin/npVideoPins.so"  },
        { "path": "plugin/npVideoPins.dll" }
    ],   
    
    "content_scripts":
    [
        {
            "matches": ["http://*.youtube.com/*", "http://youtube.com/*", "https://*.youtube.com/*", "https://youtube.com/*"],
            "js": ["/js/jquery-1.7.2.min.js", "/js/core.js", "/js/content_scripts/youtube.js"],
            "run_at": "document_end"
        },
        {
            "all_frames": true,
            "include_globs": ["http://*.youtube.com/embed/*", "http://youtube.com/embed/*", "https://*.youtube.com/embed/*", "https://youtube.com/embed/*",
                              "http://*.youtube.com/v/*", "http://youtube.com/v/*", "https://*.youtube.com/v/*", "https://youtube.com/v/*"],
            "matches": ["http://*/*", "https://*/*"],
            "js": ["/js/jquery-1.7.2.min.js", "/js/core.js", "/js/content_scripts/embed/youtube_embed.js"],
            "run_at": "document_end"
        },
        {
            "exclude_globs": [ "http://*.youtube.com/*", "http://youtube.com/*", "https://*.youtube.com/*", "https://youtube.com/*"],
            "matches": ["http://*/*", "https://*/*"],
            "js": ["/js/jquery-1.7.2.min.js", "/js/core.js", "/js/content_scripts/embed/youtube_embed_old.js"],
            "run_at": "document_end"
        }
    ],
  
    "icons":
    {
        "16": "images/icon16.png",
        "32": "images/icon32.png",
        "64": "images/icon64.png",	
        "128": "images/icon128.png"
    }
}
